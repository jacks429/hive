{
  inputs,
  nixpkgs,
  root,
}: renamer: let
  cellBlock = "modelCompression";
  l = nixpkgs.lib // builtins;
  inherit (root) walkPaisano;

  walk = self:
    walkPaisano self cellBlock (system: cell: [
      (l.mapAttrs (target: config: {
        _file = "Cell: ${cell} - Block: ${cellBlock} - Target: ${target}";
        imports = [config];
      }))
      (l.mapAttrs (_: config: {
        # Basic metadata
        name = config.name or "";
        description = config.description or "";
        
        # Model compression configuration
        sourceModel = config.sourceModel or {};
        method = config.method or "";
        parameters = config.parameters or {};
        targetSize = config.targetSize or null;
        
        # System information
        system = config.system or system;
      }))
      (l.filterAttrs (_: config: config.system == system))
    ])
    renamer;
in
  walk
